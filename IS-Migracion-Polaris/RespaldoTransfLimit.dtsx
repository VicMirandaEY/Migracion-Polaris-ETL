	,Convert(nvarchar(1),'') as	[CHARGE.ACCOUNT]
	,Convert(nvarchar(3),'') as	[MAINTAIN.TIME.CODE.BAL]
	,Convert(nvarchar(max),'') as	[LOCAL.REF]
--	INTO [STGPolaris].[dbo].[DMS-LIMIT]
FROM  [STGLegados].[dbo].[TmpLineaCreditoPasivaDia] as LCP
  LEFT OUTER JOIN [STGPolaris].[dbo].[COMPANY] as COM ON
    COM.CodCompaniaPersona = 0 -- LCP.co_compania
  LEFT OUTER JOIN [STGPolaris].[dbo].[CURRENCY] as CUR ON
    CUR.CoMonedaDatamart = LCP.co_moneda_lcp
  LEFT OUTER JOIN [STGPolaris].[dbo].[COUNTRY] as COU ON
    COU.CodPaisPersona = (SELECT MAX(co_pais) FROM [STGLegados].[dbo].[pais] as P WHERE RTRIM(LTRIM(Lower(P.nb_pais))) = RTRIM(LTRIM(Lower(LCP.nb_pais_base_lcp))))
  LEFT OUTER JOIN (SELECT GAC.co_compromiso_garantizado
                         ,STRING_AGG( ISNULL(COL.Id, ' '), '::') as CollateralCode
                   FROM  [STGLegados].[dbo].[TmpGarantiasActivasDia] as GAC
				     LEFT OUTER JOIN [STGPolaris].[dbo].[COLLATERAL.CODE] as COL ON
                       COL.CoTiColateralDatamart = GAC.ti_colateral
				   GROUP BY GAC.co_compromiso_garantizado) as COLL ON
		COLL.co_compromiso_garantizado = LCP.co_linea_credito_pasiva
 -- LEFT OUTER JOIN [STGPolaris].[dbo].[DMS-CUSTOMER-v1.0] as CUS ON
	--	CUS.[UPLOAD.COMPANY] = COM.Id
	--AND CUS.[EXTERN.SYS.ID]  = LCA.co_prestatario_lca
  LEFT OUTER JOIN [STGLegados].[dbo].[TmpClientes] as CUS ON
    CUS.Cup = LCP.co_otorgante_lcp
  LEFT OUTER JOIN [STGPolaris].[dbo].[LIMIT.REFERENCE] as LR ON
    LR.CoLimitReferenceCAF = LCP.co_linea_credito_pasiva
  WHERE LCP.co_compania = 1547  -- CAF
))
EXCEPT

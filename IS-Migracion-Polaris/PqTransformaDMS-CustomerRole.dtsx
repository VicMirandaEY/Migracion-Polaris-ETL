              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- =============================================
-- Autor:				José A. Guillen 
-- Fecha de creación:	01-12-2022
-- Descripción:			Consulta que genera los registros mapeados segun especificaciones del DMS CUSTOMER.ROLE
--						Se toma el CUSTOMER.CODE = cup + 1000000
--						
-- Archivos Origen:		personas.dbo.persona
--						
-- Historia:
-- José A. Guillén		01-12-2022	Codificación inicial
-- =============================================
--
SELECT DISTINCT
	 COM.Id as [UPLOAD.COMPANY]
	,RTRIM(LTRIM(Convert(nvarchar(10),CUS.co_persona + 1000000))) as [CUSTOMER.CODE]	-- This is the numeric code by which the Customer will be uniquely identified. The ID must be in the range 1-9999999999 (10 digits). This number can be generated automatically on input by T24, according to the value defined in the COMPANY table, or manually providing the number has not been used. 
--	,LEN(Convert(nvarchar(MAX),REL.RelationCode)) as LongitudRelCode	-- Solo para DEBUG
	,Convert(nvarchar(MAX),REL.RelationCode) as [RELATION.CODE]		-- Su valor viene de la tabla RELATION. Multivalor.
--	,LEN(Convert(nvarchar(MAX),REL.RelationCode)) as LongitudRelCustomer	-- Solo para DEBUG
	,Convert(nvarchar(MAX),REL.RelCustomer) as [REL.CUSTOMER]		-- Su valor viene de la tabla CUSTOMER. Multivalor
	,Convert(nvarchar(MAX),'') as [REL.DELIV.OPT]
--	,LEN(Convert(nvarchar(MAX),REL.RoleSujeto)) as LongitudRole		-- Solo para DEBUG
	,Convert(nvarchar(MAX),REL.RoleSujeto) as [ROLE]				-- Su valor viene de la tabla EB.ROLE. Segun mapeo este campo deberia ser Multivalue, pero no esta definido asi en DMS. Longitud de cada tramo es 20
	,Convert(nvarchar(MAX),'') as [ROLE.MORE.INFO]
	,Convert(nvarchar(MAX),'') as [ROLE.NOTES]
--INTO [STGPolaris].[dbo].[DMS-CUSTOMER.ROLE] 
FROM   [STGLegados].[dbo].[TmpCliente-x-Compania] as CXC	-- Tabla preparada con los clientes (beneficiarios) incluyendo los clientes internos
  LEFT OUTER JOIN [STGLegados].[dbo].[persona] as CUS ON
    CUS.co_persona = CXC.co_persona
  --LEFT OUTER JOIN [STGLegados].[dbo].[persona_juridica] as PJU ON
  --  PJU.co_persona_juridica = CXC.co_persona
  LEFT OUTER JOIN [STGPolaris].[dbo].[COMPANY] as COM ON	-- Tabla con valores de parámetros de Temenos mapeados a la fuente.
    COM.CodCompaniaPersona = CXC.co_compania
  LEFT OUTER JOIN (SELECT RCF.co_persona_sujeto_relacion
                         ,TC.[UPLOAD.COMPANY]
						 ,STRING_AGG( ISNULL(Substring(LTRIM(RTRIM(TEBR.Id)),1,20), ' '), '!!') as RoleSujeto
						 ,STRING_AGG( ISNULL(LTRIM(RTRIM(TR.Id)), ' '), ':') as RelationCode
						 ,STRING_AGG( ISNULL(LTRIM(RTRIM(TC.[CUSTOMER.CODE])), ' '), '::') as RelCustomer
                   FROM  [STGLegados].[dbo].[relacion] as RCF
				     LEFT OUTER JOIN [STGPolaris].[dbo].[RELATION] as TR ON
					   TR.CoTiRelacionCAF = RCF.co_ti_relacion
					 LEFT OUTER JOIN [STGPolaris].[dbo].[DMS-CUSTOMER-v1.0] as TC ON
					   TC.[EXTERN.SYS.ID] = RCF.co_persona_objeto_relacion
					 LEFT OUTER JOIN [STGPolaris].[dbo].[EB.ROLE] as TEBR ON
					   TEBR.DeRolePersona = RCF.de_cargo_rol_sujeto
				   WHERE RCF.co_persona_sujeto_relacion IN (SELECT co_persona FROM [STGLegados].[dbo].[TmpCliente-x-Compania])
				     AND RCF.co_persona_objeto_relacion IN (SELECT co_persona FROM [STGLegados].[dbo].[TmpCliente-x-Compania])
				   GROUP BY RCF.co_persona_sujeto_relacion
				           ,TC.[UPLOAD.COMPANY]) as REL ON
		REL.co_persona_sujeto_relacion = CXC.co_persona
	AND REL.[UPLOAD.COMPANY] = COM.Id
EXCEPT
SELECT * FROM [STGPolaris].[dbo].[DMS-CUSTOMER.ROLE-v1.0]</property>

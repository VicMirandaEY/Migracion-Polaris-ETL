      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{70000693-BDE1-4FE0-B772-11B7D4A7A452}"
          SQLTask:SqlStatementSource="--&#xA;-- Inicializa datos de prueba para LANGUAGE&#xA;--&#xA;TRUNCATE TABLE [STGPolaris].[dbo].[LANGUAGE] &#xA;INSERT INTO [STGPolaris].[dbo].[LANGUAGE] (Id, Mnemonic, Description, CodLanguagePersona)&#xA;SELECT  Convert(nvarchar(2),&#xA;&#x9;    CASE &#xA;&#x9;      WHEN IDI.co_idioma = 10 THEN '2' --'ES'&#xA;&#x9;      WHEN IDI.co_idioma = 11 THEN '1' --'EN'&#xA;&#x9;      --WHEN IDI.co_idioma = 12 THEN 'FR'&#xA;&#x9;      --WHEN IDI.co_idioma = 13 THEN 'IT'&#xA;&#x9;      --WHEN IDI.co_idioma = 14 THEN 'JA'&#xA;&#x9;      WHEN IDI.co_idioma = 15 THEN '3' --'PT'&#xA;&#x9;      --WHEN IDI.co_idioma = 16 THEN 'DE'&#xA;&#x9;      --WHEN IDI.co_idioma = 17 THEN 'KO'&#xA;          ELSE Convert(nvarchar(2),NULL) &#xA;&#x9;    END) as Id&#xA;       ,Convert(nvarchar(3),&#xA;&#x9;    CASE &#xA;&#x9;      WHEN IDI.co_idioma = 10 THEN 'ES'&#xA;&#x9;&#x9;  WHEN IDI.co_idioma = 11 THEN 'GB' --'ENG'&#xA;&#x9;&#x9;  --WHEN IDI.co_idioma = 12 THEN 'FRA'&#xA;&#x9;&#x9;  --WHEN IDI.co_idioma = 13 THEN 'ITA'&#xA;&#x9;&#x9;  --WHEN IDI.co_idioma = 14 THEN 'JPN'&#xA;&#x9;&#x9;  WHEN IDI.co_idioma = 15 THEN 'POR'&#xA;&#x9;&#x9;  --WHEN IDI.co_idioma = 16 THEN 'DEU'&#xA;&#x9;&#x9;  --WHEN IDI.co_idioma = 17 THEN 'KOR'&#xA;&#x9;&#x9;  ELSE Convert(nvarchar(3),NULL)  &#xA;&#x9;    END) as Mnemonic&#xA;&#x9;   ,RTRIM(LTRIM(IDI.nb_idioma)) as Description&#xA;&#x9;   ,IDI.co_idioma as CodLanguagePersona&#xA;FROM [STGLegados].[dbo].[idioma] as IDI&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

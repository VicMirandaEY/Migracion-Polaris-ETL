    <DTS:Executable
      DTS:refId="Package\Mapea a SECTOR"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{5311DE18-3F9F-49C2-8C9D-AAE04C90D9DA}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Mapea a SECTOR"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{73E9BE98-144D-4887-A1CD-FCE6E4AB9CF5}"
          SQLTask:SqlStatementSource="--&#xA;-- Cambiado el 10-05-2025  se dejan los valores en co_ti_cliente y solo se actualiza los cup en prestatarios&#xA;-- Cambiado el 05-12-2024&#xA;--&#xA;USE [STGPolaris]&#xA;GO&#xA;&#xA;TRUNCATE TABLE [STGPolaris].[dbo].[SECTOR]&#xA;--&#xA;--&#xA;--INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;--           ([Id]&#xA;--           ,[Description]&#xA;--           ,[CoTiCliente])&#xA;--     VALUES&#xA;--           ('9000'&#xA;--           ,'Other'&#xA;--           ,0) -- Se le asigna 0 cuando co_ti_cliente sea cualquier valor fuera del dominio de este campo&#xA;--GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('1001'&#xA;           ,'Persona Natural'&#xA;           ,18) -- Persona Natural&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3002'&#xA;           ,'Banco Comercial'&#xA;           ,19) -- Banco Comercial&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('2000'&#xA;           ,'***Corporate'&#xA;           ,20) -- Por Clasificar PJuridica&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3501'&#xA;           ,'Financial Corporations'&#xA;           ,21) -- Corporativo&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3004'&#xA;           ,'Banco de Desarrollo'&#xA;           ,22) -- Institución Fin. de Desarrollo&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3900'&#xA;           ,'Other Financial Institutions'&#xA;           ,23) -- Otras Instituciones Financiera&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3001'&#xA;           ,'Otras Instituciones Financieras'&#xA;           ,24) -- antes Gobierno&#xA;GO&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3003'&#x9;&#x9;&#x9;&#xA;           ,'Institucion Microfinanciera'&#xA;           ,25) -- Institución Microfinanciera&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3001'&#x9;-- Ojo está repetido&#xA;           ,'Otras Instituciones Financieras'&#xA;           ,26) -- Cooperación Técnica&#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('9999'&#xA;           ,'T24 Updates'&#xA;           ,27) -- Ejecutor &#xA;--&#xA;--&#xA;INSERT INTO [STGPolaris].[dbo].[SECTOR]&#xA;           ([Id]&#xA;           ,[Description]&#xA;           ,[CoTiCliente])&#xA;     VALUES&#xA;           ('3004'&#xA;           ,'Banco de Desarrollo'&#xA;           ,29) -- Banco de Desarrollo&#xA;--&#xA;--&#xA;USE [STGLegados]&#xA;GO&#xA;&#xA;USE [STGLegados]&#xA;GO&#xA;&#xA;--UPDATE [STGLegados].[dbo].[persona]&#xA;--   SET [STGLegados].[dbo].[persona].[co_ti_cliente] = 0&#xA;--FROM   [STGLegados].[dbo].[persona]&#xA;--GO&#xA;--&#xA;-- &#xA;UPDATE [STGLegados].[dbo].[persona] &#xA;   SET [STGLegados].[dbo].[persona].[co_ti_cliente] = TP.co_ti_cliente&#xA;FROM   [STGLegados].[dbo].[persona] as P&#xA;  INNER JOIN [STGLegados].[dbo].[prestatario] as TP ON&#xA;&#x9;TP.co_persona_prestatario = P.co_persona&#xA;--&#xA;--&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

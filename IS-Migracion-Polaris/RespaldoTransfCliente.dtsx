  LEFT OUTER JOIN [STGLegados].[dbo].[persona_juridica] as PJU ON
    PJU.co_persona_juridica = CXC.co_persona
  LEFT OUTER JOIN [STGPolaris].[dbo].[COMPANY] as COM ON	-- Tabla con valores de parámetros de Temenos mapeados a la fuente.
    COM.CodCompaniaPersona = CXC.co_compania
  LEFT OUTER JOIN [STGPolaris].[dbo].[COUNTRY] as COU ON
    COU.CodPaisPersona = CUS.co_pais_base
  LEFT OUTER JOIN [STGPolaris].[dbo].[SECTOR] as SEC ON		-- Ver comentarios abajo sobre personas naturales
    SEC.CodSectorCciPdN = PJU.co_sector_economico
  LEFT OUTER JOIN [STGPolaris].[dbo].[DEPT.ACCT.OFFICER] as OFC ON
    OFC.CodEjecutivoResponsable = CUS.co_responsable
  LEFT OUTER JOIN [STGPolaris].[dbo].[INDUSTRY] as IND ON	-- Ver comentarios abajo sobre personas naturales
    IND.CodIndustriaCAF = PJU.co_sector_institucional
  LEFT OUTER JOIN [STGLegados].[dbo].[TmpTarget-x-Cliente] as TXC ON
		TXC.co_persona = CXC.co_persona
	AND TXC.co_compania = CXC.co_compania
  LEFT OUTER JOIN [STGPolaris].[dbo].[TARGET] as TAR ON
    TAR.CodTargetCAF = TXC.CodTargetCAF
  LEFT OUTER JOIN [STGPolaris].[dbo].[COUNTRY] as NAT ON
    NAT.CodPaisPersona = CUS.co_pais_nacionalidad
  LEFT OUTER JOIN [STGLegados].[dbo].[cliente] as CLI ON
    CLI.co_persona_cliente = CUS.co_persona
  LEFT OUTER JOIN [STGPolaris].[dbo].[CUSTOMER.STATUS] as STA ON
    STA.CodCustomerStatusPdN = CLI.in_activo
  LEFT OUTER JOIN [STGPolaris].[dbo].[LANGUAGE] as LAN ON
    LAN.CodLanguagePersona = CUS.co_idioma_comunicacion
	-- STREET
  LEFT OUTER JOIN [STGLegados].[dbo].[localidad] as LST ON
        LST.co_persona_duenha_localidad = CUS.co_persona
	AND LST.nu_prioridad_localidad = 1  
    AND LST.co_ti_localidad = 11							-- 11: TRABAJO                               
  LEFT OUTER JOIN [STGLegados].[dbo].[contacto] as STT ON
        STT.co_localidad = LST.co_localidad
	AND STT.co_ti_contacto = 11								-- 11: DIRECCION, 12: HABITACION
    -- POST.CODE
  LEFT OUTER JOIN [STGLegados].[dbo].[localidad] as LPC ON
        LPC.co_persona_duenha_localidad = CUS.co_persona
	AND LPC.nu_prioridad_localidad = 1  
    AND LPC.co_ti_localidad = 11							-- 11: TRABAJO                               
  LEFT OUTER JOIN [STGLegados].[dbo].[contacto] as PCO ON
        PCO.co_localidad = LPC.co_localidad
	AND PCO.co_ti_contacto = 19								-- 11: DIRECCION, 12: HABITACION, 19:CODIGO POSTAL                           
	-- RESIDENCE
  LEFT OUTER JOIN [STGPolaris].[dbo].[COUNTRY] as RES ON
    RES.CodPaisPersona = CUS.co_pais_base

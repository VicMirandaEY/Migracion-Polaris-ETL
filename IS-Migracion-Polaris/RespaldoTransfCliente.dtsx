                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- =============================================
-- Autor:				José A. Guillen 
-- Fecha de creación:	01-12-2022
-- Descripción:			Consulta que genera los registros mapeados segun especificaciones del DMS COLLETRAL (Garantias Recibidas). 
--						
-- Archivos Origen:		datamart.dbo.garantias_activas
--						
-- Historia:
-- José A. Guillén		01-12-2022	Codificación inicial
-- =============================================
--
SELECT -- DISTINCT		-- El DISTINCT parece altera la forma de asignar los consecutivos
	   COM.Id as [UPLOAD.COMPANY]
--	  ,Convert(nvarchar(7),GAC.co_persona_prestatario+1000000)+'.'+Substring(Convert(nvarchar(5),GAC.co_garantia+10000),2,4)+'.'+Substring(Convert(nvarchar(5),GAC.co_garantia+10000),2,4) as [ID/COLLATERAL.ID]	-- 1-10 digit numeric (customer id), period, 1-4 digit numeric (sequence number, collateral right), period, 1-4 digit numeric (sequence number, object). The sequence number following the full stop may not be zero.
	  ,Convert(nvarchar(20),TCR.[ID/COLLATERAL.RIGHT]+'.'+Convert(nvarchar(4),ROW_NUMBER() OVER(PARTITION BY COM.Id, TCR.[ID/COLLATERAL.RIGHT] ORDER BY COM.Id ASC, TCR.[ID/COLLATERAL.RIGHT] ASC))) as [ID/COLLATERAL.ID]  -- 1-10 digit numeric (customer id), period, 1-4 digit numeric (sequence number, collateral right), period, 1-4 digit numeric (sequence number, object). The sequence number following the full stop may not be zero.
	  ,RTRIM(LTRIM(Convert(nvarchar(3),COL.CollateralType))) as [COLLATERAL.TYPE]	-- 1-3 digit numeric. (Mandatory input, default value determined by collateral code).
	  ,Convert(nvarchar(35),RTRIM(LTRIM(Substring(GAC.de_garantia,1,35))))+	   
	   Convert(nvarchar(35),
	   CASE
	     WHEN GAC.de_garantia_2 IS NOT NULL AND GAC.de_garantia_2 &lt;&gt; GAC.de_garantia  THEN '::'+Convert(nvarchar(35),RTRIM(LTRIM(Substring(GAC.de_garantia_2,1,35))))
		 ELSE ''
	   END) as [DESCRIPTION]
	  ,Convert(nvarchar(3),Substring(LTRIM(RTRIM(COM.Mnemonic)),1,3)) as [COMPANY]
	  ,Convert(nvarchar(34),'') as [APPLICATION.ID]
	  ,CUR.CcyId as [CURRENCY]		-- 3 alphabetic characters (currency format). (Mandatory input, default value see details).          
	  ,COU.Id as [COUNTRY]
	  ,Convert(nvarchar(19),'') as [NOMINAL.VALUE]
	  ,Convert(nvarchar(19),'') as [MAXIMUM.VALUE]
	  ,Convert(nvarchar(19),'') as [EXECUTION.VALUE]
	  ,Convert(nvarchar(19),'') as [THIRD.PARTY.VALUE]
	  ,Convert(nvarchar(19),'') as [GEN.LEDGER.VALUE]
	  ,Convert(nvarchar(19),'') as [CENTRAL.BANK.VALUE]
	  ,Convert(nvarchar(8),GAC.fe_efectiva_avaluo,112) as [VALUE.DATE]
	  ,Convert(nvarchar(17),'') as [REVIEW.DATE.FQU]
	  ,Convert(nvarchar(8),GAC.fe_vencimiento_garantia,112) as [EXPIRY.DATE]
	  ,Convert(nvarchar(max),'') as [ADDRESS]
	  ,RTRIM(LTRIM(Convert(nvarchar(3),COL.Id))) as [COLLATERAL.CODE]	-- Debe existir en la tabla de parámetros COLLATERAL.CODE. 1-3 digit numeric. (Mandatory input, default value determined by collateral code). 
	  ,Convert(nvarchar(10),'') as [COLL.RATING]
	  ,Convert(nvarchar(4),'') as [INDUSTRY]
	  ,Convert(nvarchar(max),'') as [ISSUER]
	  ,Convert(nvarchar(10),'') as [COUNTERPARTY]
	  ,Convert(nvarchar(12),'') as [EXCLUSION.ID]
	  ,Convert(nvarchar(15),'') as [PURCHASE.PRICE]
--	  ,CUS.[CUSTOMER.CODE] as [CUSTOMER.ID]
      ,RTRIM(LTRIM(Convert(nvarchar(10),CUS.Cup + 1000000))) as [CUSTOMER.ID]
	  ,TCR.[ID/COLLATERAL.RIGHT] as [COLLATERAL.RIGHT.ID]
	  ,Convert(nvarchar(max),'') as [NOTES]			-- Evaluar colocar información de referencia relativa a la migración
	  ,Convert(nvarchar(max),'') as [LOCAL.REF]
--INTO [STGPolaris].[dbo].[DMS-COLLATERAL]
FROM  [STGPolaris].[dbo].[DMS-COLLATERAL.RIGHT-v1.0] as TCR
  LEFT OUTER JOIN [STGLegados].[dbo].[TmpGarantiasActivasDia] as GAC ON
    GAC.co_garantia = TCR.co_garantia
  LEFT OUTER JOIN [STGPolaris].[dbo].[COMPANY] as COM ON
    COM.Id = TCR.[UPLOAD.COMPANY]  -- Según conversación con Oliver. Chat Teams 24/1. ¿Las Garantías se gestionan con todas las compañías (CAF, FIDE, FDH, FFC)?.... No, hasta ahora solo en CAF, el FFC jamas tendria eso y FIDE y FDH tampoco hasta el momento pero no es imposible para estas dos últimas
  LEFT OUTER JOIN [STGPolaris].[dbo].[COLLATERAL.CODE] as COL ON
    COL.CoTiColateralDatamart = GAC.ti_colateral 
  --LEFT OUTER JOIN [STGLegados].[dbo].[moneda] as MON ON
  --  MON.co_moneda = GAC.co_moneda
  LEFT OUTER JOIN [STGPolaris].[dbo].[CURRENCY] as CUR ON
    CUR.CcyId = RTRIM(LTRIM(GAC.co_moneda)) -- El co_moneda en garactias_activas no es un secuenciador, sino que representa el codigo ISO.
  LEFT OUTER JOIN [STGPolaris].[dbo].[COUNTRY] as COU ON
    COU.CodPaisPersona = (SELECT MAX(co_pais) FROM [STGLegados].[dbo].[pais] as P WHERE RTRIM(LTRIM(Lower(P.nb_pais))) = RTRIM(LTRIM(Lower(GAC.nb_pais))))
  LEFT OUTER JOIN [STGLegados].[dbo].[TmpClientes] as CUS ON
    CUS.Cup = GAC.co_persona_prestatario
 -- LEFT OUTER JOIN [STGPolaris].[dbo].[DMS-CUSTOMER-v1.0] as CUS ON
	--	CUS.[EXTERN.SYS.ID] = GAC.co_persona_prestatario
	--AND CUS.[UPLOAD.COMPANY] = COM.Id
--WHERE GAC.fe_linea_tiempo = (SELECT MAX(fe_linea_tiempo) FROM [STGLegados].[dbo].[garantias_activas])  -- No hace falta porque ya esta contemplado en la tabla TmpCollateralRight
EXCEPT
SELECT * FROM [STGPolaris].[dbo].[DMS-COLLATERAL-v1.0]
